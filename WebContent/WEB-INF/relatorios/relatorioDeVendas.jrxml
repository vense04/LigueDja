	<queryString>
		<![CDATA[SELECT
	PR.nomProduto
	,	IP.quantidade
	,	AVG(IP.preco) AS media
	,	SUM(IP.preco) AS total
FROM TAB_PEDIDO P, TAB_ITEM_PEDIDO IP, TAB_PRODUTO PR
WHERE
	P.codPedido = IP.codPedido
AND
	PR.codProduto = IP.codProduto
AND
	P.datPagamento IS NOT NULL
AND
	P.datPagamento
BETWEEN
	$P{datInicio}
AND
	$P{datFim}
GROUP BY
	PR.codProduto]]>
	</queryString>
